<h2>Flights</h2>

<ul>
  @if (Model.Count != 0)
  {
    @foreach (var flights in @Model)
    {
      <li>@flights.GetId()
        <ul>
          <li>@flights.GetArrival()</li> <li>@flights.GetDeparture()</li> <li>@flights.GetStatus()</li>
        </ul>
      </li>
    }
  }
</ul>

<p><a href="/">Home</a></p>
